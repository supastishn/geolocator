{
    "projectId": "geolocatr",
    "functions": [
        {
            "$id": "get-mapbox",
            "name": "get-mapbox",
            "runtime": "node-22",
            "specification": "s-0.5vcpu-512mb",
            "execute": [
                "any"
            ],
            "events": [],
            "scopes": [
                "users.read"
            ],
            "schedule": "",
            "timeout": 15,
            "enabled": true,
            "logging": true,
            "entrypoint": "src/main.js",
            "commands": "npm install",
            "path": "functions/get-mapbox"
        },
        {
            "$id": "gemini",
            "name": "gemini",
            "runtime": "node-22",
            "specification": "s-0.5vcpu-512mb",
            "execute": [
                "any"
            ],
            "events": [],
            "scopes": [
                "users.read"
            ],
            "schedule": "",
            "timeout": 15,
            "enabled": true,
            "logging": true,
            "entrypoint": "src/main.js",
            "commands": "npm install",
            "path": "functions/gemini",
            "environmentVariables": {
                "GEMINI_API_KEY": "your-api-key-here",
                "GEMINI_MODEL": "gemini-pro-vision"
            }
        }
    ]
